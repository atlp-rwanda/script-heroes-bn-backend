{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "engines": {
    "node": "10.19.0"
  },
  "scripts": {
    "prestart": "npm run migration:undo && npm run migrate && npm run seed",
    "start": "npm run build && node dist/index",
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "test": "nyc --reporter=html --reporter=text mocha  --timeout 60000 -r @babel/register test/*  --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migration:undo": "sequelize db:migrate:undo:all",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "admin:create": "babel-node src/database/superAdmin.js",
    "role:create": "babel-node src/database/superAdminRole.js",
    "pretest": "cross-env NODE_ENV=test && npm run migration:undo && npm run migrate && npm run seed"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.2.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.1",
    "i18n": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.10.0",
    "node-cron": "^2.0.3",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "request": "^2.87.0",
    "sendgrid": "^5.2.3",
    "sequelize": "^5.21.2",
    "sinon": "^9.2.0",
    "swagger-ui-express": "^4.1.4",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "2.1.2",
    "sequelize-cli": "^6.2.0"
  }
}
